#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    int bin[10];
    int i=0;
    while(n!=0)
    {
        int rem=n%2;
        bin[i]=rem;
        i++;
        n=n/2;
    }
    for(int j=i-1;j>=0;j--)
    {
     printf("%d", bin[j]);
    }
    return 0;
}
